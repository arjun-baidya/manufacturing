<odoo>
    <template id="filter_report_hand_consumption_id">
        <div t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <div class="oe_structure"/>
                    <t t-foreach="docs" t-as="o">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="mt16">
                                    <div>
                                        <h2 class="text-center">Hand Consumption</h2>
                                        <hr style="border: 1px solid black;"/>
                                    </div>
                                    <div class="row">
                                        <div class="col-6">
                                            <span>Series Name :
                                                <!--                                                <t t-esc="o['series_name']"/>-->
                                            </span>
                                        </div>
                                        <div class="col-6">
                                            <span>Style Name :
                                                <!--                                                <t t-esc="o['style_no']"/>-->
                                            </span>
                                        </div>
                                    </div>
                                    <table width="100%" class="table table-bordered" style="border-color:black;">
                                        <thead>
                                            <tr>
                                                <th class="text-left">Style</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <t t-foreach="merchandising_data" t-as="line">
                                                <tr>
                                                    <td style="text-align:justify">
                                                        <a>
                                                            <t t-esc="line['style_no']"/>
                                                        </a>
                                                    </td>
                                                </tr>
                                            </t>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </t>
                </div>
            </t>
        </div>
    </template>
</odoo>
