<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Sequences for Merchandising Sheet -->
        <record id="seq_merchandising_sheet" model="ir.sequence">
            <field name="name">Merchandising Sheet</field>
            <field name="code">merchandising.sheet.seq</field>
            <field name="prefix">MD</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_pattern_cut" model="ir.sequence">
            <field name="name">Pattern Cut</field>
            <field name="code">pattern.cut.sequence</field>
            <field name="prefix">PC</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>

